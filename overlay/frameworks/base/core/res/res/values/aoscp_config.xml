<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Key codes of fingerprint gestures. If the gesture is not supported set it to 0. -->
    <integer name="config_fpDoubleTapKeyCode">616</integer>
    <integer name="config_fpLongpressKeyCode">617</integer>
    <integer name="config_fpSwipeLeftKeyCode">621</integer>
    <integer name="config_fpSwipeRightKeyCode">620</integer>

    <!-- Default settings for gestures
            0 - Disabled
            1 - Toggle flashlight
            2 - Toggle airplane mode
            3 - Play/Pause music
            4 - Next music
            5 - Previous music
            6 - Take screenshot
            7 - Assistant
            8 - Home Button
            9 - Back Button
            10 - AppSwitch
            11 - Wake device -->
    <integer name="config_fpDoubleTapDefault">8</integer>
    <integer name="config_fpLongpressDefault">11</integer>
    <integer name="config_fpSwipeLeftDefault">9</integer>
    <integer name="config_fpSwipeRightDefault">10</integer>

    <!-- Whether the device supports fingerprint gestures -->
    <bool name="config_supportsFPGestures">true</bool>
	
</resources>